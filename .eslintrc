{
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-console": "warn"
    }
    
}